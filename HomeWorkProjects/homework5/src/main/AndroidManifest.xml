<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.petrusevich.volha.homework5">

    <permission
        android:name="com.gmail.petrusevich.volha.homework5.data/data.READ_DATABASE"
        android:protectionLevel="normal" />
    <permission
        android:name="com.gmail.petrusevich.volha.homework5.data/data.WRITE_DATABASE"
        android:protectionLevel="normal" />
    <permission android:name="android.permission.READ_USER_DICTIONARY"/>
    <permission android:name="android.permission.WRITE_USER_DICTIONARY"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/contacts"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/contacts_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddContactActivity" />
        <activity android:name=".EditContactActivity" />

        <provider
            android:name=".contentProvider.ContactDataContentProvider"
            android:authorities="com.gmail.petrusevich.volha.homework5"
            android:exported="true"
            android:readPermission="com.gmail.petrusevich.volha.homework5.data/data.READ_DATABASE"
            android:writePermission="com.gmail.petrusevich.volha.homework5.data/data.WRITE_DATABASE" />
    </application>

</manifest>