<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.petrusevich.volha.homework3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/contacts"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/contacts_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".contacts.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".contacts.activity.AddContactActivity"/>
        <activity android:name=".contacts.activity.EditContactActivity"/>
    </application>

</manifest>